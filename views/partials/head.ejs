.btn-toggle ,.sidebar a.btn{
  padding: .25rem 3rem;
  font-weight: 600;
  color: rgba(0, 0, 0, .65);
  letter-spacing: .5px;
  background-color: transparent;
  /*margin-left: 2.5rem !important;*/
  /*width: 80% !important;*/
}
.btn-toggle{
	width: 100%;
}
.btn-toggle:hover, .sidebar a.btn:hover,
.btn-toggle:focus {
  color: rgba(0, 0, 0, .85);
  background-color: #d2f4ea;
}
.sidebar{
	position: fixed;
	z-index: 1 !important;
}
.sidebar a.btn{
	letter-spacing: .5px;
}
.btn-toggle::before {
  width: 1.25em;
  line-height: 0;
  content: url("data:image/svg+xml,%3csvg xmlns='http://www.w3.org/2000/svg' width='16' height='16' viewBox='0 0 16 16'%3e%3cpath fill='white' stroke='rgba%280,0,0,.5%29' stroke-linecap='round' stroke-linejoin='round' stroke-width='2' d='M5 14l6-6-6-6'/%3e%3c/svg%3e");
  transition: transform .35s ease;
  transform-origin: .5em 50%;
}
.btn-toggle-0::before{
  width: 1.25em;
  line-height: 0;
  content: url();
  transition: transform .35s ease;
  transform-origin: .5em 50%;
}

.btn-toggle[aria-expanded="true"],.sidebar a.btn, .sidebar button {
  color: #ddd;
}
.btn-toggle[aria-expanded="true"]::before {
  transform: rotate(90deg);
}

.btn-toggle-nav a{
  color: #ddd !important;
  padding: .1875rem .5rem;
  margin-top: .125rem;
  margin-left: 1.25rem;
}
.btn-toggle-nav a:hover,.sidebar a.btn:hover,
.btn-toggle-nav a:focus {
  background-color: #d2f4ea;
}

.scrollarea {
  overflow-y: auto;
}
#list-unstyled li{
	color: #ddd;
}
.sidebar .fa, .sidebar .bi{
	position: absolute;
	left: 5px;
	margin-right: 5px;
}
.sidebar li a.btn, .sidebar li button.btn{
	padding-top: 10px;
	padding-bottom: 10px;
}
